{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "getting_started/dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build_getting_started",
            "program": "${workspaceFolder}/scenarios/getting_started/dotnet/getting_started/bin/Debug/net7.0/getting_started.dll",
            "args": [],
            "cwd": "${workspaceFolder}/scenarios/getting_started/dotnet/getting_started",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "launchSettingsProfile": "cloud"
        },
        {
            "name": "telemetry_producer/dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build_telemetry",
            "program": "${workspaceFolder}/scenarios/telemetry/dotnet/telemetry_producer/bin/Debug/net7.0/telemetry_producer.dll",
            "args": [],
            "cwd": "${workspaceFolder}/scenarios/telemetry/dotnet/telemetry_producer",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "launchSettingsProfile": "cloud"
        },
        {
            "name": "telemetry_consumer/dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build_telemetry",
            "program": "${workspaceFolder}/scenarios/telemetry/dotnet/telemetry_consumer/bin/Debug/net7.0/telemetry_consumer.dll",
            "args": [],
            "cwd": "${workspaceFolder}/scenarios/telemetry/dotnet/telemetry_consumer",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "launchSettingsProfile": "cloud"
        },
        {
            "name": "command_consumer/dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build_command",
            "program": "${workspaceFolder}/scenarios/command/dotnet/command_consumer/bin/Debug/net7.0/command_consumer.dll",
            "args": [],
            "cwd": "${workspaceFolder}/scenarios/command/dotnet/command_consumer",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "launchSettingsProfile": "cloud"
        },
        {
            "name": "command_producer/dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build_command",
            "program": "${workspaceFolder}/scenarios/command/dotnet/command_producer/bin/Debug/net7.0/command_producer.dll",
            "args": [],
            "cwd": "${workspaceFolder}/scenarios/command/dotnet/command_producer",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "launchSettingsProfile": "cloud"
        },
        {
            "name": "C Telemetry Producer",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/scenarios/telemetry/c/build/scenarios/telemetry/c/telemetry_producer",
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "envFile": "${workspaceRoot}/scenarios/telemetry/telemetry_producer.env",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build C Telemetry",
            "presentation": {
                "hidden": true,
                "group": "C"
            }
        },
        {
            "name": "C Telemetry Consumer",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/scenarios/telemetry/c/build/scenarios/telemetry/c/telemetry_consumer",
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "envFile": "${workspaceRoot}/scenarios/telemetry/telemetry_consumer.env",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build C Telemetry",
            "presentation": {
                "hidden": true,
                "group": "C"
            }
        },
        {
            "name": "C Getting Started",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/scenarios/getting_started/c/build/scenarios/getting_started/c/getting_started",
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "envFile": "${workspaceRoot}/scenarios/getting_started/getting_started.env",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build C Getting Started",
            "presentation": {
                "group": "C"
            }
        },
    ],
    "compounds": [
        {
            "name": "telemetry/dotnet",
            "configurations": ["telemetry_producer/dotnet", "telemetry_consumer/dotnet" ]
        },
        {
            "name": "command/dotnet",
            "configurations": ["command_producer/dotnet", "command_consumer/dotnet" ]
        },
        {
            "name": "C Telemetry",
            "configurations": ["C Telemetry Consumer", "C Telemetry Producer" ],
            "stopAll": true,
            "presentation": {
                "group": "C"
            }
        }
    ]
}
